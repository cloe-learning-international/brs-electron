{
  "name": "brs-electron",
  "version": "1.3.2",
  "description": "Electron App For BRS",
  "keywords": [
    "Bootstrap",
    "Electron",
    "ES6",
    "ECMAScript 6"
  ],
  "repository": "https://github.com/roseaubenjamin/brs-electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "build",
    "publish": "build --win -p always",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . brs-electron-app --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"brs-electron\"",
    "package-linux": "electron-packager . brs-electron-app --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Benjamin Roseau",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-builder": "^20.44.4",
    "electron-packager": "^14.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "ajv": "^6.10.2",
    "auto-launch": "^5.0.5",
    "bootstrap": "^4.3.1",
    "bootstrap-input-spinner": "^1.11.8",
    "copy-dir": "^1.2.0",
    "csv-parser": "^2.3.0",
    "csv-write-stream": "^2.0.0",
    "electron-json-storage": "^4.1.6",
    "electron-package": "^0.1.0",
    "electron-reload": "^1.4.1",
    "electron-settings": "^2.2.4",
    "electron-simple-updater": "^1.5.0",
    "electron-updater": "^4.1.2",
    "infusionsoft": "^0.3.7",
    "infusionsoft-api": "^0.6.0",
    "infusionsoft-node-sdk": "0.0.1",
    "jquery": "^3.4.1",
    "passport-infusionsoft": "0.0.3",
    "popper.js": "^1.15.0",
    "readdir-recursive": "0.0.4",
    "recursive-readdir-async": "^1.1.5",
    "tablesort": "^5.1.0",
    "update-electron-app": "^1.5.0",
    "v": "^0.3.0"
  },
  "build": {
    "appId": "BrsElectron.V1.0.0",
    "productName": "BRS-Electron",
    "copyright": "Webero Agency Ltd 2019",
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "publish": {
      "provider": "github"
    }
  }
}
